{
  "name": "@rxts/koa",
  "version": "0.0.0",
  "description": "Incredible Koa middlewares/plugins, make Koa greater",
  "repository": "git@github.com:rx-ts/koa-router-decorators.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -p",
    "build:ts": "tsc -b",
    "dev": "nodemon",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext md,ts",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "type-coverage": "type-coverage --cache --detail --ignore-catch --ignore-files '**/*.d.ts' --strict"
  },
  "devDependencies": {
    "@1stg/lib-config": "^0.1.21",
    "@1stg/tslint-config": "^0.5.9",
    "@koa/router": "^8.0.6",
    "@types/http-proxy": "^1.17.3",
    "@types/koa": "^2.11.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-session": "^5.10.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^13.5.0",
    "consola": "^2.11.3",
    "koa": "^2.11.0",
    "koa-compose": "^4.1.0",
    "koa-session": "^5.12.3",
    "lerna": "^3.20.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "type-coverage": "^2.4.0",
    "yarn-deduplicate": "^1.1.1"
  },
  "resolutions": {
    "@babel/core": "^7.8.3",
    "eslint-plugin-prettier": "^3.1.2",
    "typescript": "^3.7.5"
  },
  "nodemonConfig": {
    "ext": ".ts",
    "exec": "ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}' -T server",
    "ignore": "packages/lib",
    "watch": [
      "packages",
      "server"
    ]
  },
  "typeCoverage": {
    "atLeast": 93
  }
}
